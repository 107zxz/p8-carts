__lua__
function _init()

	-- persistent data
	cartdata"depth_perception"
	
	-- reset button
	menuitem(5,"reset save",function()
		dset(0,0)
		dset(1,0)
		dset(2,0)
	end)
	
	-- inverted circle
	poke(0x5f34, 0x2)
	
	-- palette
	poke(0x5f2e,1)
	pal({[1]=-14,[3]=-3,[4]=-16,[10]=-9,[12]=-1}, 1 )
	palt(0x0010)

	local px,py=dget(0),dget(1)
	p={
		x=px!=0 and px or 60,
		y=py!=0 and py or 103,
		f=px>=60,
		s=px<60 and 1 or 34
	}
	
	-- hide cage if no loadout
	if dget(2) == 0 then
		for i=6,9 do
			mset(i,7,0)
		end
	end
end

function _update()
	update_player()
	
	-- cutscene
	update_cutscene()
	update_dialogue()
end

function _draw()
	cls()
	map()
	spr(p.s,p.x,p.y,1,2,p.f)
	
	-- npcs
--	spr(34,56,56,1,2)
	
	-- darkness
	circfill(
		64,64,
		68+sin(t()/5)*4,0x1800
	)
	
	draw_dialogue()

	-- debug
--	print(
--		"{"..p.x..","..p.y.."}",
--		0,0,
--		13
--	)
end
