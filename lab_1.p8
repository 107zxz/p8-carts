-- player
function update_player()

	-- don't move during dialogue
	if #dial_queue!=0 then
		return
	end
	
	
	local dx,dy=0,0

	dx+=btn(âž¡ï¸) and 1 or 0
	dx-=btn(â¬…ï¸) and 1 or 0

	dy+=btn(â¬‡ï¸) and 1 or 0
	dy-=btn(â¬†ï¸) and 1 or 0
	
	p.f=p.f and not (dx>0) 
					or (dx<0) 
	
	p.s=(dx!=0) and 1
					or dy>0 and 33
					or dy<0 and 34
	    or p.s

	local mtile=mget(
		(p.x+dx*4+4)\8,
		(p.y+dy*5+10)\8
	)
	if mtile == 2 or mtile==10 then
		p.x+=dx
		p.y+=dy
	end
end
