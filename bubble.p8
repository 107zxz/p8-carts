-- persistent data! stays when
-- rebooting (level switch)
--- level progress
level=1

function _init()
	balls={}
	for k,v in pairs(lvlballs[level]) do
		balls[k]={}
		for k2,v2 in pairs(lvlballs[level][k]) do
			balls[k][k2]=v2
		end
	end
	
	ballpals={
		{[8]=1,[14]=2},
		{[8]=9,[14]=10},
		{[8]=3,[14]=11},
		{[8]=14,[14]=15}
	}
	
	-- {x,y,st}
	booms={}
	
	-- wall
	wspin=64
	wopening=8
	
	-- soul pos (follows wall)
	soulx=64
	
	-- flash
	screenflash=0
	
	-- held ball
	heldball=false
	
	-- lives
	lives=5
	
	-- pause game
	inmenu=true
	
	-- death/restart overlay
	ddx=1
	dprg=1
	
	-- input repeat delays
	poke(0x5f5c,3)
	poke(0x5f5d,3)
	
	-- mouse input
	-- pointer lock, enable, buttons
	poke(0x5f2d,0x7)
end

function _update()

	if inmenu do
	 update_menu()
	 return
	end

	-- screenflash
	screenflash-=1

 -- ball physics
 for ball in all(balls) do
		update_ball(ball)
	end
	
	-- booms
	for boom in all(booms) do
		if boom.st<t()-0.15 do
			del(booms,boom)
		end
	end
	
	-- soul follow wall
	soulx=(soulx+wspin)\2
	
	-- wall movement
	if (btn(âž¡ï¸)) wspin+=3
	if (btn(â¬…ï¸)) wspin-=3
	
	-- mouse wall movement
	wspin+=stat(38)/8
	
	-- clamp
	wspin=mid(8,wspin,120)
	
	-- end conditions
	if lives <= 0 and screenflash<0 do
		_init()
	end
	
	-- todo: optimize this one
	if #booms>0 do
		return
	end
	
	for ball in all(balls) do
	 if ball.p!=99 do
	 	return
	 end
	end
	
	-- we're ready to move on!
	
	for i=1,30 do
	 -- todo: do a static or smth
		cls()
		flip()
	end
	
	level+=1
	_init()
end

function draw_background()
	-- fancy background
	fillp(â–‘)
	rectfill(0,0,128,128,1)
	fillp()
	circfill(64,196,168+16*sin(t()/16),0)
	
	-- environment
	local bx,by=64,72+sin(t()/4)*4
	rect(bx-12,by-12,bx+12,by+12,1)
	rect(bx-6,by-8,bx+6,by+12,1)
	pset(bx+4,by+2,1)
	line(bx-12,by+12,0,96,1)
	line(bx+12,by+12,128,96,1)
	line(bx-12,by-12,0,0,1)
	line(bx+12,by-12,128,0,1)
end

function _draw()
	cls()
	
	if not inmenu do
	 draw_background()
	end
	
	-- soul barrier
	for i=0,15 do
		spr(1,i*8,88+sin(t()+i/2))
	end
	rectfill(wspin-wopening,90,wspin+wopening,94,0)
	
	-- eggfx
	fillp(â–‘)
	circ(soulx,111,14+sin(t()/2)*3,8)
	fillp()
	
	-- egg
	spr(2,soulx-4+sin(t())*2,108+cos(t())*2)
	spr(2,soulx-4-sin(t())*2,108)
	spr(2,soulx-4,108-cos(t())*2)

	-- bouncie balls
	for ball in all(balls) do
		drawball(ball)
	end
	
	-- booms
	fillp(âˆ§)
	for boom in all(booms) do
		circ(boom.x,boom.y,64*(t()-boom.st)+8,7)
	end
	fillp()
	
	-- screen fx
	camera()
	if screenflash>0 do
		pal(1,8)
		camera(sin(screenflash/4)*4+2)
--		cls(8)
	else
	 pal()
	end
	
	-- life counter
	for i=1,lives do
		print("â™¥",i*7-7,122,8)
	end
	
	-- menu
	if inmenu do
	 draw_menu()
	end
	
--	draw_chevron()
end
