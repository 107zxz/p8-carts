function draw_chevron()
 local lvcnt=4
 local cvsz=3
 
 local cv="\fd\^od10"
 for i=1,lvcnt do
  if i==level do
  	cv=cv.."\f7\^o710"
  elseif i>level do
  	cv=cv.."\f0\^od42"
  end
 	cv=cv..rpt("â–ˆ",cvsz)..chr(23)
 end
 
 cv=cv.." \^o000\f7ðŸ±"
 
	print(cv,0,0)
end

function rpt(c,x)
  ret=""
  for i=1,x do
   ret=ret..c
  end
  return ret
end
